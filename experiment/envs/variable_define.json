{
  "USP": "$Cell culture total$+$Replacement ATF filters$",
  "Total costs": "$Seed$+$USP$+$DSP$+$Changeover$+$Storage$+$Backlog penalties$+$Wastage$",
  "Profit": "$Revenue$-$Total costs$",
  "Profit_PER_DAY": "$Profit$/$ALL_DAYS$",
  "DELTA_PROFIT_PER_DAY": "$DELTA_PROFIT$/$DAYS$",
  "ALL_ORDER_PER_DAY": "$ALL_ORDER_DICT$[$P$]/$ALL_DAYS$",
  "ALL_ORDER": "$ALL_ORDER_DICT$[$P$]",
  "FIT_ORDER_PER_DAY": "$FIT_ORDER_DICT$[$P$]/$ALL_DAYS$",
  "FIT_ORDER": "$FIT_ORDER_DICT$[$P$]",
  "NOT_FIT_ORDER": "$NOT_FIT_ORDER_DICT$[$P$]",
  "INVENTORY": "$INVENTORY_DICT$[$P$]",
  "TOTAL_INVENTORY": "sum($INVENTORY_DICT$.values())",
  "TOTAL_NOT_FIT_ORDER": "sum($NOT_FIT_ORDER_DICT$.values())",
  "TOTAL_FIT_ORDER": "sum($FIT_ORDER_DICT$.values())",
  "TOTAL_ALL_ORDER": "sum($ALL_ORDER_DICT$.values())",
  "TOTAL_PRODUCTION": "$TOTAL_FIT_ORDER$+$TOTAL_INVENTORY$",
  "DECAY": "$DECAY_DICT$[$P$]",
  "CSL_P": "$CSL_DICT$[$P$]",
  "SALES_VOLUME_P": "$SALES_VOLUME_DICT$[$P$]",
  "UNIT_PRICE_P": "$UNIT_PRICE_DICT$[$P$]",
  "CSL_REWARD": "100 * $IDLE_PERCENT$ * (100 - $DELTA_CSL$) * $DELTA_CSL$",
  "INVENTORY_REWARD": "- ($INVENTORY$ / 10 * ($CSL_P$ ))",
  "PRICE_COE": "(1 + (1 - $CSL_P$) * $UNIT_PRICE_P$ / 20)",
  "REWARD_1": "$DELTA_PROFIT_PER_DAY$ * $PRICE_COE$",
  "INVENTORY_MINUS_NOT_FIT_ORDER": "$INVENTORY$-$NOT_FIT_ORDER$"
}
